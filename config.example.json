{
  "cloudflare": {
    "email": "your-email@example.com",
    "api_token": "your-cloudflare-api-token-here"
  },
  "schedule": {
    "day_start_hour": 6,
    "day_end_hour": 22,
    "day_ip": "1.2.3.4",
    "night_ip": "5.6.7.8",
    "_comment": "时间使用本地时间，白天6:00-22:00使用day_ip，夜间22:00-6:00使用night_ip"
  },
  "domains": [
    {
      "name": "example.com",
      "zone": "example.com",
      "type": "A"
    },
    {
      "name": "www.example.com",
      "zone": "example.com",
      "type": "A"
    },
    {
      "name": "api.example.com",
      "zone": "example.com",
      "type": "A"
    }
  ],
  "auto_discovery": true,
  "log": {
    "level": "INFO",
    "file": "/var/log/cloudflare-auto-ddns.log"
  },
  "check_interval": 300
}
